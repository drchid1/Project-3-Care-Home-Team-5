   "source": [
    "# Save to Excel with color coding\n",
    "output_file_path = \"../data/final_nh_data_with_ratings.xlsx\"\n",
    "\n",
    "with pd.ExcelWriter(output_file_path, engine='openpyxl') as writer:\n",
    "    df_unique_dy.to_excel(writer, index=False, sheet_name='Care Homes')\n",
    "    workbook = writer.book\n",
    "    worksheet = writer.sheets['Care Homes']\n",
    "    \n",
    "    # Define fill styles\n",
    "    fill_good = PatternFill(start_color=\"00FF00\", end_color=\"00FF00\", fill_type=\"solid\")\n",
    "    fill_requires_improvement = PatternFill(start_color=\"FFFF00\", end_color=\"FFFF00\", fill_type=\"solid\")\n",
    "    fill_no_published_rating = PatternFill(start_color=\"FF0000\", end_color=\"FF0000\", fill_type=\"solid\")\n",
    "    fill_inadequate = PatternFill(start_color=\"FF69B4\", end_color=\"FF69B4\", fill_type=\"solid\")\n",
    "    fill_outstanding = PatternFill(start_color=\"ADD8E6\", end_color=\"ADD8E6\", fill_type=\"solid\")\n",
    "    \n",
    "    # Iterate over rows and apply fill styles\n",
    "    for idx, row in df_unique_dy.iterrows():\n",
    "        cell = worksheet.cell(row=idx + 2, column=df_unique_dy.columns.get_loc('Rating') + 1)\n",
    "        if row['Rating'] == 'Good':\n",
    "            cell.fill = fill_good\n",
    "        elif row['Rating'] == 'Requires improvement':\n",
    "            cell.fill = fill_requires_improvement\n",
    "        elif row['Rating'] == 'No published rating':\n",
    "            cell.fill = fill_no_published_rating\n",
    "        elif row['Rating'] == 'Inadequate':\n",
    "            cell.fill = fill_inadequate\n",
    "        elif row['Rating'] == 'Outstanding':\n",
    "            cell.fill = fill_outstanding\n",
    "            \n",
    "print(\"Data has been successfully exported to:\", output_file_path)"
   ]
